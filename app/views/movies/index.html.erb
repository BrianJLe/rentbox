<table>
	<tr>
		<th>Title</th>
		<th>Genre</th>
		<th>Rating</th>
		<th>Inventory</th>
	</tr>
	<% @movies.each do |movie| %>
	<tr>
		<td> <%= movie.title %> </td>
		<td> <%= movie.genre %> </td>
		<td> <%= movie.rating %> </td>
		<td> <%= movie.inventory %> </td>
	</tr>
	<% end %>
</table>

<li><%= link_to "Rent A Movie", new_rental_path %></li>
<li><%= link_to "Return A Movie", new_return_path  %></li>